<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE OSU [
<!ELEMENT OSU (beatmaps) >
<!ATTLIST OSU version CDATA #REQUIRED>

<!ELEMENT beatmaps (beatmap*)>
<!ATTLIST beatmaps count CDATA #REQUIRED>

<!ELEMENT beatmap EMPTY>
<!ATTLIST beatmap name CDATA #REQUIRED artist CDATA #REQUIRED 
difficulty CDATA #REQUIRED bestGrade CDATA #IMPLIED>
]>


